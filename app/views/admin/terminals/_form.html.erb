<div class="row">
  <div class="col-lg-12">
    <section class="panel">
      <header class="panel-heading">
         Form Elements
      </header>
      <div class="panel-body">
        <%= simple_form_for [:admin, @terminal], :defaults => { :input_html => { :class => "form-control" } }, html: { multipart: true, class: 'form-horizontal tasi-form' } do |f| %> 

            <%= f.input :mac %>
            <%= f.input :imei %>
            <%= f.input :sim_iccid %>
            
            <div class="form-actions">
              <%= f.button :submit %>
            </div>
        <% end %>
      </div>
    </section>
  </div>
</div>
