<%= simple_form_for [:admin, @merchant] do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :industry %>
    <%= f.input :province %>
    <%= f.input :city %>
    <%= f.input :area %>
    <%= f.input :circle %>
    <%= f.input :address %>
    <%= f.input :contact %>
    <%= f.input :mobile %>
    <%= f.input :secondary %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
