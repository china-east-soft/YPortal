  <div class="row">
      <div class="col-lg-12">
          <section class="panel">
              <header class="panel-heading">
                  广告列表
              </header>
              <div class="panel-body">
                <div class="clearfix">
                    <div class="btn-group">
                        <span class="btn btn-primary" >
                          <%= link_to fa_icon("plus"), new_admin_landing_path %>
                        </span>
                    </div>
                </div>
                <div class="space15"></div>

                <table class="table table-striped table-advance table-hover">
                  <thead>
                    <tr>
                      <th>Start at</th>
                      <th>End at</th>
                      <th>Url</th>
                      <th>Cover</th>
                      <th colspan="3"></th>
                    </tr>
                  </thead>

                  <tbody>
                    <% @landings.each do |landing| %>
                      <tr>
                        <td><%= landing.start_at %></td>
                        <td><%= landing.end_at %></td>
                        <td><%= landing.url %></td>
                        <td><%= landing.cover %></td>
                        <td>
                          <span class="btn btn-success btn-xs"><%= link_to fa_icon("check"), [:admin, landing] %></span>
                          <span class="btn btn-primary btn-xs"><%= link_to fa_icon("pencil"), edit_admin_landing_path(landing) %><i class="icon-pencil"></i></span>
                          <span class="btn btn-danger btn-xs"><%= link_to fa_icon("trash-o"), [:admin, landing], method: :delete, data: { confirm: 'Are you sure?' } %></span>
                        </td>
                      </tr>
                    <% end %>
                  </tbody>
              </table>
              </div>
          </section>
      </div>
  </div>
