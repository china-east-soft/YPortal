<div class="row">
  <div class="col-lg-12">
    <section class="panel">
      <header class="panel-heading">
        <%= t(".form_elements") %>
      </header>
      <div class="panel-body">
        <%= simple_form_for [:merchant, @mbox], remote: true, :defaults => { :input_html => { :class => "form-control" } }, html: { multipart: true, class: 'form-horizontal tasi-form' } do |f| %>

          <%= f.input :category, disabled: true %>
          <%= f.input :name %>

          <div class="form-actions">
            <%= f.button :submit %>
          </div>
        <% end %>
      </div>
    </section>
  </div>
</div>

